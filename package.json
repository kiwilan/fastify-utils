{
  "name": "@kiwilan/fastify-utils",
  "version": "0.0.1",
  "description": "Fastify utility.",
  "author": "Ewilan Rivi√®re <ewilan.riviere@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/kiwilan/vite-plugin-laravel-steward",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiwilan/vite-plugin-laravel-steward.git"
  },
  "bugs": {
    "url": "https://github.com/kiwilan/vite-plugin-laravel-steward/issues"
  },
  "keywords": [
    "steward",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./build.js": {
      "import": "./src/build/build.js",
      "require": "./src/build/build.js"
    },
    "./build.ts": {
      "import": "./src/build/esbuild.ts",
      "require": "./src/build/esbuild.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "directories": {
    "dist": "dist",
    "lib": "lib"
  },
  "scripts": {
    "build": "tsup --clean",
    "watch": "tsup --watch",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "esbuild": "^0.16.12",
    "esbuild-plugin-pino": "^1.2.8",
    "fastify": "^4.10.2",
    "ofetch": "^1.0.0",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@types/node": "^18.11.16",
    "eslint": "^8.30.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
